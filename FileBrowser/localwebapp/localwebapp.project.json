{
  "$schema": "https://nuscien.github.io/winkit/schema/localwebapp.project.v6.json",
  "package": {
    "title": "File Browser",
    "publisher": "Nanchang Jinchen Software Co Ltd",
    "icon": ".\\images\\icons\\Logo.png",
    "description": "Explore local files.",
    "copyright": "Copyright Â© 2022 Kingcean Tuan, all rights reserved.",
    "website": "https://trivial.kingcean.net"
  },
  "ref": {
    "update": {
      "url": "http://localhost/install/Test/LocalWebApp.json"
    },
    "updateMeta": {
      "path": ".\\temp\\LocalWebApp.json",
      "urlTemplate": "http://localhost/install/Test/filebrowser_{ver}.zip",
      "hash": "SHA256"
    },
    "path": "..\\bin\\LocalWebApp\\filebrowser\\app",
    "sign": "RS256"
  },
  "dev": {
    "copy": [
      {
        "src": "..\\index.html",
        "file": true,
        "dist": "index.html"
      },
      {
        "src": "..\\css",
        "dist": "css"
      },
      {
        "src": "..\\dist",
        "dist": "dist"
      },
      {
        "src": "..\\images",
        "dist": "images"
      },
      {
        "src": "..\\data",
        "dist": "data"
      }
    ]
  }
}